{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/backend/api/index.ts"
    },
    {
      "source": "/health",
      "destination": "/backend/api/index.ts"
    }
  ]
}
